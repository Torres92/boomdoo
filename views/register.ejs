<!DOCTYPE html>
  <html lang="es" ng-app="app">
    <head>
      <!--Let browser know website is optimized for mobile-->
      <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
      <!--Import Google Icon Font-->
      <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
      <!--Import materialize.css-->
      <link type="text/css" rel="stylesheet" href="lib/materialize/materialize.min.css"  media="screen,projection"/>

      <link rel="stylesheet" href="css/register.css">

    </head>

    <body ng-controller="CtrlRegister">

      <section class="main">
        <div class="row">
            <div class="col m2 l2"></div>
            <div class="col s11 m8 l8 card-panel login">
              <div class="row">
                <div class="col s12">
                  <ul class="tabs tabs-fixed-width">
                    <li class="tab col s3"><a class="active black-text" href="#test1">Estudiante</a></li>
                    <li class="tab col s3"><a class="black-text" href="#test2">Profesor</a></li>
                  </ul>
                </div>

                
                <div id="test1" class="col s12">
                  <div class="card vertical">
                    <div class="row">
                      <div class="col s4"></div>
                      <div class="col s4 fluid-img card-image">
                        <img class="logobrand responsive-img" src="img/boomboo.png">
                      </div>
                      <div class="col s4"></div>
                    </div> 
                    <div class="card-stacked">
                      <div class="row">
                        <div class="col s12 card-content">
                          <div class="row">
                            <form class="col s12" ng-submit="">

                              <div class="row">
                                <div class="input-field col s6">
                                  <input id="first_name" type="text" class="validate" required ng-model="user.name">
                                  <label for="first_name">Nombre</label>
                                </div>
                                <div class="input-field col s6">
                                  <input id="last_name" type="text" class="validate" required ng-model="user.lastname">
                                  <label for="last_name">Apellido</label>
                                </div>
                              </div>

                              <div class="row">
                                <div class="col s12 m6 l6">
									<div class="row">	
										<div class="input-field col s6">	
	                                      	<label>
	                                        <input value="masculino" name="group1" type="radio" ng-model="user.sex" class="with-gap "/>
	                                        <span class="">Masculino</span>
	                                      	</label>
	                                    </div> 
	                                	<div class="input-field col s6"> 
		                                  <label>
		                                    <input value="femenino" name="group1" type="radio" ng-model="user.sex" class="with-gap"/>
		                                    <span>Femenino</span>
		                                  </label>
		                                </div>
	                                </div>	
                                </div>
                                     
                                 
                                
                                <div class="input-field col s12 m6 l6">
                                  <input type="text" id="fechaNacimiento" class="datepicker" ng-model="user.birthday">
                                  <label for="fechaNacimiento">Fecha de nacimiento</label>
                                </div>
                              </div>
                              

                              <div class="row">
                                <div class="input-field col s6">
                                  <input id="ocupacion" type="text" class="validate">
                                  <label for="ocupacion">Ocupación</label>
                                </div>
                                <div class="input-field col s6">
                                  <input id="paisOrigen" type="text" class="validate">
                                  <label for="paisOrigen">País de Ubicación</label>
                                </div>
                              </div>

                              <div class="row">
                                <div class="input-field col s12 m6 l6">
                                  <input id="email" type="email" class="validate">
                                  <label for="email">Email</label>
                                </div>
                                <div class="input-field col s12 m6 l6">
                                  <div class="file-field input-field">
                                    <div class="btn-small blue darken-4">
                                      <span><i class="material-icons">file_upload</i></span>
                                      <input type="file">
                                    </div>
                                    <div class="file-path-wrapper">
                                      <input class="file-path validate" placeholder="Foto de Perfil(Opcional)" type="text">
                                    </div>
                                  </div>
                                </div>
                              </div>

                              <div class="row">
                              <div class="input-field col s12 m6 l6">
                                  <input id="password" type="password" class="validate">
                                  <label for="password">Contraseña</label>
                                </div>
                                <div class="input-field col s12 m6 l6">
                                  <input id="pass" type="password" class="validate">
                                  <label for="pass">Repetir Contraseña</label>
                                </div>
                                <div class="col s6">
                                </div>
                              </div>
                              
                              <div class="row">
                                <div class="col s6 card-action">
                                  <div class="row">
                                    <div class="col s4 hide-on-med-and-down"></div>
                                    <div class="col s12 m4 l4 center"> 
                                      <a class="waves-effect red lighten-2 waves-light btn-small" href="/">atras</a>
                                    </div>
                                    <div class="col s4 hide-on-med-and-down"></div>

                                   </div> 
                               
                                </div>
                                <div class="col s6 card-action">
                                  <div class="row">
                                    <div class="col s4 hide-on-med-and-down"></div>
                                    <div class="col s12 m4 l4 center"> 
                                      <button class="btn blue darken-4 waves-effect waves-light" type="submit" name="action">Registrar
                                        <i class="material-icons right">send</i>
                                      </button>
                                    </div>
                                    <div class="col s4 hide-on-med-and-down"></div>
                                    
                                  </div>
                                </div>
                                <div class="col s12 center">  
                                  <span class="helper-text">Al Registrarte, estas aceptando nuestros <a class="center" href="#">Términos y Condiciones.</a> </span>
                                </div>
                              </div>

                            </form>
                          </div>
                        </div>
                      </div>

                    </div>
                  </div>
                </div>


                <div id="test2" class="col s12">
                  <div class="card vertical">
                    <div class="row">
                      <div class="col s4"></div>
                      <div class="col s4 fluid-img card-image">
                        <img class="logobrand responsive-img" src="img/boomboo.png">
                      </div>
                      <div class="col s4"></div>
                    </div> 
                    <div class="card-stacked">
                      <div class="row">
                        <div class="col s12 card-content">
                          <div class="row">
                            <form class="col s12">

                              <div class="row">
                                <div class="input-field col s6">
                                  <input id="Nombre" type="text" class="validate">
                                  <label for="Nombre">Nombre</label>
                                </div>
                                <div class="input-field col s6">
                                  <input id="Apellido" type="text" class="validate">
                                  <label for="Apellido">Apellido</label>
                                </div>
                              </div>

                              <div class="row">
                                <div class="col s12 m6 l6">
                                  <div class="row">
                                    <div class="input-field col s6">

                                      <label>
                                        <input name="group1" type="radio" class="with-gap"/>
                                        <span>Masculino</span>
                                      </label>
                                    </div>
                                    <div class="input-field col s6"> 
                                      <label>
                                        <input name="group1" type="radio" class="with-gap"/>
                                        <span>Femenino</span>
                                      </label>
                                    </div>  
                                  </div>
                                </div>

                                <div class="input-field col s12 m6 l6">
                                  <input type="text" id="Nacimiento" class="datepicker">
                                  <label for="Nacimiento">Fecha de nacimiento</label>
                                </div>
                              </div>
                              

                              <div class="row">
                                <div class="input-field col s6">
                                  <input id="Ocu" type="text" class="validate">
                                  <label for="Ocu">Ocupación</label>
                                </div>
                                <div class="input-field col s6">
                                  <input id="paisUbi" type="text" class="validate">
                                  <label for="paisUbi">País de Ubicación</label>
                                </div>
                              </div>

                              <div class="row">
                                <div class="input-field col s12 m6 l6">
                                  <input id="e-mail" type="email" class="validate">
                                  <label for="e-mail">Email</label>
                                </div>
                                <div class="input-field col s12 m6 l6">
                                  <div class="file-field input-field">
                                    <div class="btn-small blue darken-4">

                                      <span><i class="material-icons">file_upload</i></span>
                                      <input type="file">
                                    </div>
                                    <div class="file-path-wrapper">
                                      <input class="file-path validate" placeholder="Foto de Perfil(Opcional)" type="text">
                                    </div>
                                  </div>
                                </div>
                              </div>

                              

                              <div class="row">
                                <div class="input-field col s12">
                                  <textarea id="textarea2" class="materialize-textarea" data-length="250"></textarea>
                                  <label for="textarea2">Breve Presentacion</label>
                                  <span class="helper-text">*Escriba aquí una breve descripción sobre usted, algún dato relevante, algún dato
                                  de importancia, esto será estudiado para su postulación en esta página.</span>
                                </div>
                              </div>

                              <div class="row">
                                <h5 class="center">Áreas de Competencia:</h4>
                                <div class="input-field center col s6">
                                    <label>
                                      <input type="checkbox" class="filled-in"/>
                                      <span>Básica</span>
                                    </label> 
                                </div>
                                <div class="input-field center col s6">
                                    <label>
                                      <input type="checkbox" class="filled-in"/>
                                      <span>Diversificada</span>
                                    </label> 
                                </div>
                                <div class="input-field center col s6">
                                    <label>
                                      <input type="checkbox" class="filled-in"/>
                                      <span>Universitario</span>
                                    </label> 
                                </div>
                                <div class="input-field center col s6">
                                    <label>
                                      <input type="checkbox" class="filled-in"/>
                                      <span>Asesoría</span>
                                    </label> 
                                </div>
                                <div class="input-field center col s6">
                                    <label>
                                      <input type="checkbox" class="filled-in"/>
                                      <span>Conversación</span>
                                    </label> 
                                </div>
                                <br><br>
                                <div class="input-field file-field col s12">
                                  <div class="file-path-wrapper">
                                    <input class="file-path validate" type="text" placeholder="Adjunte los documentos que avalen sus conocimientos">
                                    <input type="file" multiple>
                                  </div>
                                  <div class="btn blue darken-3">
                                    <span>Adjuntar</span>
                                  </div>
                                  <span class="helper-text">*Nota: Este campo no es obligatorio, pero es importante para el análisis de su postulación.</span>
                                </div>
                              </div>

                              
                              <div class="row">
                                <h5 class="center">Usuario:</h2>
                              </div>

                              <div class="row">
                                <div class="input-field col s12 m6 l6">
                                  <input id="passw" type="password" class="validate">
                                  <label for="passw">Contraseña</label>
                                </div>
                              </div>
                              
                              <div class="row">
                                <div class="input-field col s12 m6 l6">
                                  <input id="passwor" type="password" class="validate">
                                  <label for="passwor">Repetir Contraseña</label>
                                </div>
                                <div class="col s6">
                                </div>
                              </div>
                              
                        
                              <div class="row">
                                <div class="col s6 card-action">
                                  <div class="row">
                                    <div class="col s4 hide-on-med-and-down"></div>
                                    <div class="col s12 m4 l4 center"> 
                                      <a class="waves-effect red lighten-2 waves-light btn-small" href="/">atras</a>
                                    </div>
                                    <div class="col s4 hide-on-med-and-down"></div>

                                   </div> 
                               
                                </div>
                                <div class="col s6 card-action">
                                  <div class="row">
                                    <div class="col s4 hide-on-med-and-down"></div>
                                    <div class="col s12 m4 l4 center"> 
                                      <button class="btn blue darken-4 waves-effect waves-light" type="submit" name="action">Registrar
                                        <i class="material-icons right">send</i>
                                      </button>
                                    </div>
                                    <div class="col s4 hide-on-med-and-down"></div>
                                    
                                  </div>
                                </div>
                                <div class="col s12 center">  
                                  <span class="helper-text">Al Registrarte, estas aceptando nuestros <a class="center" href="#">Términos y Condiciones.</a> </span>
                                </div>
                              </div>

                            </form>
                          </div>
                        </div>
                      </div>

                    </div>
                  </div>
                </div>  
              </div>
            </div>
            <div class="col m2 l2"></div>
        </div>
      </section>

</body>
    <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.6.10/angular.min.js"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
    <script src="lib/materialize/materialize.min.js"></script>
    <script src="js/config/configRegister.js"></script>
    <script src="js/services/Auth.js"></script>
    <script src="js/controllers/CtrlRegister.js"></script>
    <script src="js/materialize/register.js"></script>

  </html>