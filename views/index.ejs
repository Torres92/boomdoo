      <!-- Modal student -->

      <!-- Modal Structure -->
        <div id="modal1" class="modal maxWidth">
          <div class="modal-content contenedorModal">
          	<div class="progress barraloginEspera" ng-show="preloadStudent">
		          <div class="indeterminate"></div>
		        </div>
            <form  class="modalForm" ng-submit="loginStudent(user,remember)">
              <div class="imgcontainer modalIMG">
                <img src="img/student.png" alt="Avatar" class="avatar">
              </div>
              <div class="alert-danger col red white-text" ng-show="messageStudent">
                <h6 class="center-align">{{messageStudent}}</h6>
              </div>
              <div class="row">
                <div class="input-field col s12">
                  <input placeholder="Correo Electronico" id="usuarioLogin" type="email" class="validate" required ng-model="user.email">
                  <label for="usuarioLogin">Correo Electronico:</label>
                </div>
              </div>

              <div class="row">
                <div class="input-field col s12">
                  <input placeholder="Introduzca Contraseña" id="passwordLogin" type="password" class="validate" required ng-model="user.password">
                  <label for="passwordLogin">Contraseña</label>
                </div>
              </div>

              <button class="login btn blue darken-4 waves-effect waves-light" type="submit" ng-disabled="preloadStudent">Iniciar Sesión</button>
              <label class="checkboxModal">
                  <input type="checkbox" class="filled-in" ng-model="remember"/>
                  <span>Recordar contraseña</span>
              </label>

              <div class="contenedor">
                <button type="button" class="cancelbtn white-text modal-close">Cancelar</button>
                <a class="psw">Olvidaste tu <b href="#">contraseña?</b></a>
              </div>
            </form>
          </div>         
        </div>

      <!-- Modal teacher -->

      <!-- Modal Structure -->
        <div id="modal2" class="modal maxWidth">
          <div class="modal-content contenedorModal">
            <div class="progress barraloginEspera" ng-show="preloadTeacher">
              <div class="indeterminate"></div>
            </div>
            <form  class="modalForm" ng-submit="loginTeacher(usertwo,remembertwo)">
              <div class="imgcontainer modalIMG">
                <img src="img/professor.png" alt="Avatar" class="avatar">
              </div>
              <div class="alert-danger col red white-text" ng-show="messageTeacher">
                <h6 class="center-align">{{messageTeacher}}</h6>
              </div>
              <div class="row">
                <div class="input-field col s12">
                  <input placeholder="Correo Electronico"  type="email" class="validate" required ng-model="usertwo.email">
                  <label for="usuarioLogin">Correo Electronico:</label>
                </div>
              </div>

              <div class="row">
                <div class="input-field col s12">
                  <input placeholder="Introduzca Contraseña" type="password" class="validate" required ng-model="usertwo.password">
                  <label for="passwordLogin">Contraseña</label>
                </div>
              </div>

              <button class="login btn blue darken-4 waves-effect waves-light" type="submit" ng-disabled="preloadTeacher">Iniciar Sesión</button>
              <label class="checkboxModal">
                  <input type="checkbox" class="filled-in" ng-model="remembertwo"/>
                  <span>Recordar contraseña</span>
              </label>

              <div class="contenedor">
                <button type="button" class="cancelbtn white-text modal-close">Cancelar</button>
                <a class="psw">Olvidaste tu <b href="#">contraseña?</b></a>
              </div>
            </form>
          </div>         
        </div>


